{
  "name": "galahad/laravel-addressing",
  "description": "Laravel package providing addressing functionality",
  "keywords": [
    "laravel",
    "address",
    "addressing",
    "validation",
    "countries"
  ],
  "homepage": "https://github.com/glhd/laravel-addressing",
  "authors": [
    {
      "name": "Chris Morrell"
    },
    {
      "name": "Junior Grossi",
      "email": "juniorgro@gmail.com",
      "homepage": "http://github.com/jgrossi"
    }
  ],
  "require": {
    "php": ">=5.6",
    "illuminate/support": "5.3.*|5.4.*|5.5.*",
    "commerceguys/addressing": "^1.0",
    "symfony/intl": "^3.3.6",
    "commerceguys/intl": "^0.7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5",
    "orchestra/testbench": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "Galahad\\LaravelAddressing\\": "src/"
    }
  },
  "license": "MIT",
  "extra": {
    "laravel": {
      "providers": [
        "Galahad\\LaravelAddressing\\ServiceProvider"
      ],
      "aliases": {
        "Addressing": "Galahad\\LaravelAddressing\\AddressFacade"
      }
    }
  }
}
